{
    "name": "Haskell Workspace",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "haskell-workspace",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "streetsidesoftware.code-spell-checker",
                "mhutchie.git-graph",
                "ms-azuretools.vscode-docker",
                "haskell.haskell"
            ]
        }
    }
}
